Version: 1.0

RestoreWorkspace: Default
SaveWorkspace: Default
AlwaysSaveHistory: Default

EnableCodeIndexing: Yes
UseSpacesForTab: Yes
NumSpacesForTab: 2
Encoding: UTF-8

RnwWeave: Sweave
LaTeX: pdfLaTeX

BuildType: Package
PackageUseDevtools: Yes
PackageInstallArgs: --no-multiarch --with-keep.source
PackageBuildArgs: -O3 -ffast-math -funroll-loops
PackageBuildBinaryArgs: -O3 -ffast-math -funroll-loops
PackageCheckArgs: -g -O3 -ffast-math -funroll-loops
PackageRoxygenize: rd,collate,namespace,vignette
